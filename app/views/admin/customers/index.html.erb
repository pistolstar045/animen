<h2><span class="bg-light">会員一覧<span></h2>
 <table class='table'>
  <thead class="table table-light">
   <tr>
    <th>会員ID</th>
    <th>氏名</th>
   </tr>
  </thead>
  <tbody>
   <% @customers.each do |customer| %>
    <tr>
     <td><%= customer.id %></td>
     <td><%= link_to customer.name,admin_customer_path(customer.id) %></td>
    </tr>
   <% end %>
　</tbody>
 </table>
 
 
